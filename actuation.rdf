        <rdfs:comment xml:lang="en">Defines allowable or required structures for key-value lists. Used in interface definitions.</rdfs:comment>
        <rdfs:label xml:lang="en">Key-Value definition</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/rec/actuation/ResponseCode -->

