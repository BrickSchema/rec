  <owl:Ontology rdf:about="">
    <cc:license rdf:resource="https://opensource.org/licenses/MIT"/>
    <dc:description xml:lang="en">This REC module covers the actuation model, i.e., south- and northbound message syntaxes for requesting and enacting actuation on building systems of various types, and the interfaces/payload schemas associated with such systems.</dc:description>
    <vann:preferredNamespaceUri>https://w3id.org/rec/actuation/3.1.1/</vann:preferredNamespaceUri>
    <vann:preferredNamespacePrefix>actuation</vann:preferredNamespacePrefix>
    <dc:creator>Karl Hammar</dc:creator>
    <owl:versionInfo xml:lang="en">3.1.1</owl:versionInfo>
    <owl:imports rdf:resource="https://w3id.org/rec/device/3.1.1/"/>
    <dc:creator>Per Karlberg</dc:creator>
    <owl:versionIRI rdf:resource="https://w3id.org/rec/actuation/3.1.1/"/>
    <terms:modified>2020-04-21</terms:modified>
    <dc:publisher>RealEstateCore Consortium</dc:publisher>
    <dc:title>RealEstateCore Actuation Module</dc:title>
  </owl:Ontology>
  <owl:Class rdf:about="https://w3id.org/rec/actuation/ActuationRequest">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="https://w3id.org/rec/actuation/hasRequestResponse"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
